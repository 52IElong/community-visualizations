{
  "name": "modular",
  "main": "index.js",
  "config": {
    "comment": "---- Edit these config values as needed ----",
    "gcs_dev_bucket": "YOUR_DEV_BUCKET",
    "gcs_prod_bucket": "YOUR_PROD_BUCKET",
    "js_file": "index.js",
    "json_file": "index.json",
    "manifest_file": "manifest.json"
  },
  "scripts": {
    "build:dev": "node scripts/build.js dev",
    "build:prod": "node scripts/build.js prod",
    "push:dev": "(cd ./dist && gsutil cp -a public-read * $npm_package_config_gcs_dev_bucket)",
    "push:prod": "(cd ./dist && gsutil cp -a public-read * $npm_package_config_gcs_prod_bucket)"
  },
  "devDependencies": {
    "webpack-merge": "4.1.4",
    "copy-webpack-plugin": "^4.5.3",
    "webpack-plugin-replace": "^1.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@google/dscc": "0.1.3",
    "d3": "5.7.0",
    "d3-sankey": "^0.7.1"
  }
}
